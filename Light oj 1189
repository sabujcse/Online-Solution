#include<bits/stdc++.h>
using namespace std;
long long fact[21]={1,1,2,6,24,120,720,5040,40320,362880,3628800,39916800,479001600,6227020800,87178291200,1307674368000,20922789888000,355687428096000,6402373705728000,121645100408832000,2432902008176640000};
int main()
{
    int test, c= 1;
    scanf("%d",&test);
    while( test-- )
    {
        int p[20];
        int k = 0;
        long long n;
      scanf("%lld",&n);
      for( int j = 20; j>=0; j-- )
      {
          if( n>=fact[j])
          {
              n=n-fact[j];
              p[k++]=j;
          }
      }
    printf("Case %d: ",c++);
    if( n==0 )
    {
        for( int z = k-1; z>=0; z-- )
        {
            if( z == k-1 )
                printf("%d!",p[z]);
            else
                printf("+%d!",p[z]);
        }
        printf("\n");
    }
    else
    printf("impossible\n");
    }
    return 0;
}
